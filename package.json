{
  "name": "remote-manager",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "postinstall": "npm link @lianmed/components @lianmed/request @lianmed/hooks @lianmed/im @lianmed/pages",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "fetch:blocks": "pro fetch-blocks --branch antd@4 && npm run prettier",
    "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "dev": "cross-env UMI_UI=none umi dev",
    "dev:no-mock": "cross-env UMI_UI=none MOCK=none umi dev",
    "start:ui": "umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "prod": "yarn build && node index.js",
    "start": "node index.js",
    "ui": "umi ui"
  },
  "husky": {
    "hooks": {
      "1pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0-alpha.19",
    "@ant-design/pro-layout": "^5.0.0",
    "@ant-design/pro-table": "^2.0.0",
    "@antv/data-set": "^0.10.2",
    "@types/lodash.isequal": "^4.5.5",
    "@types/react-router": "^5.0.2",
    "@types/store": "^2.0.2",
    "ali-oss": "^6.5.1",
    "antd": "^4.1.4",
    "bizcharts": "^3.5.3-beta.0",
    "china-division": "^2.3.1",
    "classnames": "^2.2.6",
    "dva": "^2.6.0-beta.16",
    "easemob-websdk": "^3.0.7",
    "gg-editor": "^3.1.0",
    "jsoneditor": "^8.6.4",
    "koa": "^2.11.0",
    "koa-better-http-proxy": "^0.2.4",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.0",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "prop-types": "^15.5.10",
    "qs": "^6.9.0",
    "query-string": "^6.11.1",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-crons": "^2.2.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "store": "^2.0.12",
    "sunflower-antd": "^0.9.1",
    "umi": "^2.13.0",
    "umi-plugin-antd-icon-config": "^1.0.2",
    "umi-plugin-antd-theme": "^1.0.1",
    "umi-plugin-pro-block": "^1.3.2",
    "umi-plugin-react": "^1.14.10",
    "umi-request": "^1.0.8",
    "use-merge-value": "^1.0.1",
    "use-state-with-callback": "^1.0.18"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.17",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.13",
    "@umijs/fabric": "2.0.0",
    "chalk": "^3.0.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.0",
    "cross-port-killer": "^1.1.1",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.0.7",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^9.0.0",
    "mockjs": "^1.0.1-beta3",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "pro-download": "1.0.1",
    "stylelint": "^13.0.0",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-pro": "^1.0.2",
    "umi-types": "^0.5.9"
  },
  "optionalDependencies": {
    "puppeteer": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
